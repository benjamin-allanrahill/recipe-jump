{
  "manifest_version": 2,
  "name": "Recipe Jump",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.0.min.js", "content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": {
      "16": "popup/static/16.png",
      "32": "popup/static/32.png",
      "64": "popup/static/64.png",
      "128": "popup/static/128.png"
    },
    "default_popup": "popup/static/popup.html",
    "default_title": "Recipe Jump"
  },
  "icons": {
    "16": "popup/static/16.png",
    "32": "popup/static/32.png",
    "64": "popup/static/64.png",
    "128": "popup/static/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
